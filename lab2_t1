#include <stdio.h>  
#include <string.h>  
void strinv(char *st){  
    char c;  
 int len = strlen(st);  
 for(int i; i < len/2; i++){  
        c = st[i];  
 st[i] = st[len-1-i];  
 st[len-1-i] = c;  
 }  
}  
  
int main() {  
    char a[50];  
 scanf("%s", a);  
 strinv(a);  
 printf("%s", a);  
 return 0;  
}
